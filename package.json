  "private": true,
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "dependencies": {
    "@akosasante/react-auth-context": "github:akosasante/react-auth-context",
    "@mui/icons-material": "^5.14.3",
    "@mui/material": "^5.14.3",
    "@mui/styled-engine-sc": "^5.12.0",
    "@reduxjs/toolkit": "^1.9.5",
    "@stripe/react-stripe-js": "^2.1.2",
    "@stripe/stripe-js": "^2.1.0",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^14.0",
    "@testing-library/user-event": "^14.4",
    "axios": "^1.4.0",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-payment-inputs": "^1.1.9",
    "react-redux": "^8.1.2",
    "react-router-dom": "^6.15.0",
    "react-scripts": "5.0.1",
    "redux": "^4.2.1",
    "styled-components": "^5.3.11",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "craco start",
    "build": "craco build",
    "test": "craco test --transformIgnorePatterns 'node_modules/(?!axios)/' --detectOpenHandles --silent",
    "test:ci": "craco test --watchAll=false --transformIgnorePatterns 'node_modules/(?!axios)/' --detectOpenHandles --silent",
    "eject": "react-scripts eject",
    "format:all": "npx prettier --write .",
    "format:check": "npx prettier --check .",
    "lint:fix": "eslint --fix src",
    "lint:check": "eslint src"
  },
  "eslintConfig": {
    "extends": [
      "react",
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@craco/craco": "^7.1.0",
    "eslint-config-prettier": "^8.9.0",
    "husky": "^8.0.3",
    "miragejs": "^0.1.47",
    "prettier": "3.0.0"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "jest": {
    "moduleNameMapper": {
      "@mui/styled-engine": "@mui/styled-engine-sc"
    }
  }
}
